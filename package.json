{
  "name": "@4s1/snow-white-ui",
  "version": "1.0.0",
  "description": "Snow White - UI",
  "bugs": {
    "url": "https://gitlab.com/4s1/snow-white/snow-white-ui/issues"
  },
  "repository": "gitlab:4s1/snow-white/snow-white-ui",
  "license": "MIT",
  "author": "Steffen <steffen@4s1.de>",
  "scripts": {
    "build": "SKIP_PREFLIGHT_CHECK=true react-scripts build",
    "preinstall": "npx only-allow pnpm",
    "eject": "react-scripts eject",
    "format": "prettier --write \"src/**/*.ts\"",
    "lbt": "npm run lint && npm run build && npm run test",
    "lint": "eslint --ext .ts src/",
    "lint:fix": "npm run lint -- --fix",
    "start": "PORT=3001 SKIP_PREFLIGHT_CHECK=true react-scripts start",
    "test": "echo no tests available"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": "@4s1/eslint-config",
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "dependencies": {
    "@4s1/snow-white-shared": "4.0.0",
    "@fortawesome/fontawesome-svg-core": "1.2.36",
    "@fortawesome/free-solid-svg-icons": "5.15.4",
    "@fortawesome/react-fontawesome": "0.1.15",
    "@testing-library/react": "11.2.7",
    "@testing-library/user-event": "13.2.1",
    "axios": "0.21.4",
    "bootstrap": "4.6.0",
    "moment": "2.29.1",
    "moment-timezone": "0.5.33",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router-dom": "5.2.1",
    "react-scripts": "4.0.3",
    "react-select": "4.3.1",
    "typescript": "4.4.2",
    "web-vitals": "1.1.2"
  },
  "devDependencies": {
    "@4s1/eslint-config": "3.1.1",
    "@types/node": "14.17.15",
    "@types/react": "17.0.20",
    "@types/react-dom": "17.0.9",
    "@types/react-router-dom": "5.1.8",
    "@types/react-select": "4.0.17",
    "eslint": "7.32.0",
    "prettier": "2.4.0"
  },
  "engines": {
    "node": ">=14"
  }
}
