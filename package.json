{
  "name": "@4s1/snow-white-ui",
  "version": "1.1.1",
  "private": true,
  "description": "Snow White - UI",
  "bugs": {
    "url": "https://gitlab.com/4s1/snow-white-ui/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/4s1/snow-white-ui.git"
  },
  "license": "MIT",
  "author": "Steffen <steffen@4s1.de>",
  "type": "commonjs",
  "scripts": {
    "build": "SKIP_PREFLIGHT_CHECK=true react-scripts build",
    "eject": "react-scripts eject",
    "format": "prettier --write \"src/**/*.ts\"",
    "lbt": "npm run lint && npm run build && npm run test",
    "lint": "eslint --ext .ts src/",
    "lint:fix": "npm run lint -- --fix",
    "prepare": "husky install",
    "release": "git diff --exit-code --quiet && pnpm run lbt && standard-version",
    "release:major": "pnpm run release -- --release-as major",
    "release:minor": "pnpm run release -- --release-as minor",
    "release:patch": "pnpm run release -- --release-as patch",
    "start": "PORT=3001 SKIP_PREFLIGHT_CHECK=true react-scripts start",
    "test": "jest",
    "test:cov": "pnpm run test -- --coverage",
    "test:watch": "pnpm run test -- --watch"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": "@4s1/eslint-config",
  "dependencies": {
    "@4s1/snow-white-shared": "5.1.0",
    "@fortawesome/fontawesome-svg-core": "1.2.36",
    "@fortawesome/free-solid-svg-icons": "5.15.4",
    "@fortawesome/react-fontawesome": "0.1.16",
    "@testing-library/react": "12.1.2",
    "@testing-library/user-event": "13.5.0",
    "axios": "0.24.0",
    "bootstrap": "4.6.1",
    "moment": "2.29.1",
    "moment-timezone": "0.5.34",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router-dom": "5.3.0",
    "react-scripts": "4.0.3",
    "react-select": "4.3.1",
    "web-vitals": "1.1.2"
  },
  "devDependencies": {
    "@4s1/eslint-config": "3.4.0",
    "@4s1/ts-config": "1.4.0",
    "@commitlint/cli": "15.0.0",
    "@commitlint/config-conventional": "15.0.0",
    "@types/jest": "27.0.3",
    "@types/node": "14.18.2",
    "@types/react": "17.0.37",
    "@types/react-dom": "17.0.11",
    "@types/react-router-dom": "5.3.2",
    "@types/react-select": "4.0.18",
    "eslint": "8.5.0",
    "husky": "7.0.4",
    "jest": "27.4.5",
    "prettier": "2.5.1",
    "standard-version": "9.3.2",
    "ts-jest": "27.1.2",
    "typescript": "4.5.4"
  },
  "engines": {
    "node": ">=14"
  }
}
