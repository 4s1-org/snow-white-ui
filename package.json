{
  "name": "@yellowgarbagebag/snow-white-ui",
  "version": "1.0.0",
  "private": true,
  "description": "Snow White UI",
  "homepage": "https://gitlab.com/yellow-garbage-group/snow-white",
  "bugs": {
    "url": "https://gitlab.com/yellow-garbage-group/snow-white/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:yellow-garbage-group/snow-white.git"
  },
  "license": "ISC",
  "author": "Steffen",
  "scripts": {
    "build": "react-scripts build",
    "preinstall": "npx only-allow pnpm",
    "eject": "react-scripts eject",
    "format": "prettier --write \"src/**/*.ts\"",
    "lbt": "npm run lint && npm run build && npm run test",
    "lint": "eslint --ext .ts src/",
    "lint:fix": "npm run lint -- --fix",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "test:cov": "react-scripts test --coverage --watchAll=false"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": "@hest-lab/eslint-config",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.8.3",
    "@yellowgarbagebag/snow-white-dto": "^2.0.1",
    "@yellowgarbagebag/snow-white-shared": "workspace:*",
    "axios": "^0.21.1",
    "bootstrap": "^4.6.0",
    "moment-timezone": "^0.5.33",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.0",
    "react-select": "^3.2.0",
    "typescript": "^3.9.9",
    "web-vitals": "^0.2.4"
  },
  "devDependencies": {
    "@hest-lab/eslint-config": "^1.5.0",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.37",
    "@types/react": "^16.14.5",
    "@types/react-dom": "^16.9.12",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-select": "^3.1.2",
    "eslint": "^7.23.0",
    "prettier": "^2.2.1"
  }
}
